<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.SignupController">
   <children>
      <AnchorPane prefHeight="90.0" prefWidth="700.0" style="-fx-background-color: #212121;">
         <children>
            <Label alignment="CENTER" layoutX="220.0" layoutY="18.0" prefHeight="55.0" prefWidth="260.0" text="ToDo" textFill="WHITESMOKE">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="308.0" layoutY="103.0" text="Sign Up" textFill="#212121">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="signupFirstName" layoutX="24.0" layoutY="155.0" prefHeight="25.0" prefWidth="296.0" promptText="First Name" />
      <TextField fx:id="signupLastName" layoutX="380.0" layoutY="155.0" prefHeight="25.0" prefWidth="296.0" promptText="Last Name" />
      <TextField fx:id="signupUsername" layoutX="24.0" layoutY="200.0" prefHeight="25.0" prefWidth="296.0" promptText="Username" />
      <TextField fx:id="signupLocation" layoutX="380.0" layoutY="247.0" prefHeight="25.0" prefWidth="296.0" promptText="Location" />
      <CheckBox fx:id="signupCheckBoxMale" layoutX="61.0" layoutY="251.0" mnemonicParsing="false" selected="true" text="Male" />
      <CheckBox fx:id="signupCheckBoxFemale" layoutX="143.0" layoutY="251.0" mnemonicParsing="false" text="Female" />
      <PasswordField fx:id="signupPassword" layoutX="380.0" layoutY="200.0" prefHeight="25.0" prefWidth="296.0" promptText="Password" />
      <Button fx:id="signupButton" layoutX="305.0" layoutY="316.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="90.0" style="-fx-background-color: #00796B;" text="Sign Up" textFill="WHITESMOKE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
